            <string>///CONTROLLER
scr_input()

switch (state) {
    case owletStates.idle:
        scr_owlet_idle()
        break
    case owletStates.run:
        scr_owlet_run()
        break
    case owletStates.jump:
        scr_owlet_jump()
        break
}


//PHYSICS
if (moveY &lt; 10)
{
    moveY = min(moveY + grav, 9);
}

//COLLISION CHECK
if (place_meeting(x, y + moveY, o_ground))
{
    while (not place_meeting(x, y + sign(moveY), o_ground))
    {
        y += sign(moveY);                  
    }
            
